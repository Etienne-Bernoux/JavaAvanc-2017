package fr.ece.bernoux.Lab6Lambda.Exercice3;

public class Main {

	public static void main(String[] args) {
		Seq<String> seq = new <String>Seq.Cons("hello ",new <String>Seq.Cons("world!",new <String>Seq.NIi()));
		seq.forEach(System.out::print);
		
		seq = Seq.nil();
		Seq seq2 = seq.prepend("world").prepend("\nhello");
		seq2.forEach(System.out::println);

	}

}
